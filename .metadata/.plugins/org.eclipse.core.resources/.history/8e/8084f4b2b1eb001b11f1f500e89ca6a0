package WebScraper;

import java.util.List;
import java.util.ArrayList;

public class InfoSinottico {
	
	private String categoria;
	private List<Informazione> info;
	
	private class Informazione {
		private String nomeInfo;
		private String url;
		
		public Informazione(String nomeInfo) {
			this.nomeInfo = nomeInfo;
			this.url = null;
		}
		
		public void setUrl(String url) {
			//TODO controllo validità url
			this.url = url;
		}
		
		public String getNomeInfo() {
			return nomeInfo;
		}
		
		public String getUrl() {
			return url;
		}
	}

}
